{
    "name": "nvtr05/omnipay-onepay",
    "type": "library",
    "description": "Thư viện hổ trợ tích hợp cổng thanh toán OnePay.",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "omnipay/common": "^3.0"
    },
    "require-dev": {
        "omnipay/tests": "^3.0",
        "scrutinizer/ocular": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Omnipay\\OnePay\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omnipay\\OnePay\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "\"vendor/bin/phpunit\""
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
	}
}
